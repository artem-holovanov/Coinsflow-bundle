{
  "name": "coninsflow/common-bundle",
  "description": "A reusable Common Symfony Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Artem Holovanov"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "6.4.*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^10.5",
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "CommonBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CommonBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  }
}